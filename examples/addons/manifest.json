{
  "name": "My Awesome Homepage",
  "version": "1.0",
  "manifest_version": 2,
  "description": "My very own browser homepage",
  "homepage_url": "https://www.womenwhocode.com/manila",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "permissions": [
    "bookmarks"
  ]
}
